<XML-FSCM-INVOICE-2003A>
  <INTERCHANGE>
    <IC-SENDER>
      <Pid>{{herkunft_nummer}}</Pid>
    </IC-SENDER>
    <IC-RECEIVER>
      <Pid>{{enkunde_nummer}}</Pid>
    </IC-RECEIVER>
    <IR-Ref />
  </INTERCHANGE>
  <INVOICE>
    <HEADER>
      <FUNCTION-FLAGS>
        <Confirmation-Flag />
        <Canellation-Flag />
      </FUNCTION-FLAGS>
      <MESSAGE-REFERENCE>
        <REFERENCE-DATE>
          <Reference-No>{{rechnung_datum_timestamp}}</Reference-No>
          <Date>{{rechnung_datum}}</Date>
        </REFERENCE-DATE>
      </MESSAGE-REFERENCE>
      <PRINT-DATE>
        <Date>{{rechnung_datum}}</Date>
      </PRINT-DATE>
      <REFERENCE>
        <INVOICE-REFERENCE>
          <REFERENCE-DATE>
            <Reference-No>{{rechnung_nummer}}</Reference-No>
            <Date>{{rechnung_datum}}</Date>
          </REFERENCE-DATE>
        </INVOICE-REFERENCE>
        <ORDER>
          <REFERENCE-DATE>
            <Reference-No>{{auftrag_numer}}</Reference-No>
            <Date>{{rechnung_datum}}</Date>
          </REFERENCE-DATE>
        </ORDER>
        <REMINDER Which="MAH">
          <REFERENCE-DATE>
            <Reference-No></Reference-No>
            <Date></Date>
          </REFERENCE-DATE>
        </REMINDER>
        <OTHER-REFERENCE Type="ADE">
          <REFERENCE-DATE>
            <Reference-No>12345678</Reference-No>
            <Date>{{rechnung_datum}}</Date>
          </REFERENCE-DATE>
        </OTHER-REFERENCE>
      </REFERENCE>
      <BILLER>
        <Tax-No>{{herkunft_mwst}}</Tax-No>
        <Doc-Reference Type="ESR-ALT "></Doc-Reference>
        <PARTY-ID>
          <Pid>{{herkunft_nummer}}</Pid>
        </PARTY-ID>
        <NAME-ADDRESS Format="COM">
          <NAME>
            <Line-35>{{herkunft_name}}</Line-35>
            <Line-35>{{herkunft_addresse}}</Line-35>
            <Line-35>{{herkunft_ort}}</Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </NAME>
          <STREET>
            <Line-35></Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </STREET>
          <City></City>
          <State></State>
          <Zip></Zip>
          <Country></Country>
        </NAME-ADDRESS>
        <BANK-INFO>
          <Acct-No></Acct-No>
          <Acct-Name></Acct-Name>
          <BankId Type="BCNr-nat" Country="CH">001996</BankId>
        </BANK-INFO>
      </BILLER>
      <PAYER>
        <PARTY-ID>
          <Pid>{{enkunde_nummer}}</Pid>
        </PARTY-ID>
        <NAME-ADDRESS Format="COM">
          <NAME>
            <Line-35>{{enkunde_name}}</Line-35>
            <Line-35>{{enkunde_addresse}}</Line-35>
            <Line-35>{{enkunde_ort}}</Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </NAME>
          <STREET>
            <Line-35></Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </STREET>
          <City></City>
          <State></State>
          <Zip></Zip>
          <Country></Country>
        </NAME-ADDRESS>
      </PAYER>
    </HEADER>
    <LINE-ITEM />
    <SUMMARY>
      <INVOICE-AMOUNT>
        <Amount>{{total_amount}}</Amount>
      </INVOICE-AMOUNT>
      <VAT-AMOUNT>
        <Amount></Amount>
      </VAT-AMOUNT>
      <DEPOSIT-AMOUNT>
        <Amount></Amount>
          <REFERENCE-DATE>
            <Reference-No></Reference-No>
            <Date></Date>
          </REFERENCE-DATE>
      </DEPOSIT-AMOUNT>
      <EXTENDED-AMOUNT Type="79">
        <Amount></Amount>
      </EXTENDED-AMOUNT>
      <TAX>
        <TAX-BASIS>
          <Amount></Amount>
        </TAX-BASIS>
        <Rate Categorie="S">0</Rate>
        <Amount></Amount>
      </TAX>
      <PAYMENT-TERMS>
        <BASIC Payment-Type="ESR" Terms-Type="1">
          <TERMS>
            <Payment-Period Type="M" On-Or-After="1" Reference-Day="31">{{rechnung_zahlungsziel}}</Payment-Period>
            <Date>{{rechnung_datum}}</Date>
          </TERMS>
        </BASIC>
        <DISCOUNT Terms-Type="22">
          <Discount-Percentage>0.0</Discount-Percentage>
          <TERMS>
            <Payment-Period Type="M" On-Or-After="1" Reference-Day="31"></Payment-Period>
            <Date></Date>
          </TERMS>
          <Back-Pack-Container Encode="Base64"> </Back-Pack-Container>
        </DISCOUNT>
      </PAYMENT-TERMS>
    </SUMMARY>
  </INVOICE>
</XML-FSCM-INVOICE-2003A>